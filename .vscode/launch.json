{
  "version": "0.2.0",
  "configurations": [
    {
        "type": "chrome",
        "request": "launch",
        "name": "Vue JS Debug",
        "url": "http://localhost:5173",
        "webRoot": "${workspaceFolder}/src",
        "sourceMapPathOverrides": {
            "webpack://src/*": "${webRoot}/*"
        }
    },
    // DONT USE THESE! Just for reference
    {
      "name": "Next.js: debug server-side",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev"
    },
    {
      "name": "Next.js: debug client-side",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:3000"
    },
    {
      "name": "Next.js: debug full stack",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev",
      "serverReadyAction": {
        "pattern": "- Local:.+(https?://.+)",
        "uriFormat": "%s",
        "action": "debugWithChrome"
      }
    }
  ]
}
